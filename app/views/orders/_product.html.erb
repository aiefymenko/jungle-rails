<% @order.line_items.each do |item| %>
<tr>
<td> 
  <%= item.id %>
</td>
  <td>
    <%= image_tag item.product.image.thumb.url %>
  </td>
  <td>
    <h4><%= item.product.name %></h4>
    <br>
    <p><%= item.product.description %></p>
  </td>
  <td>
  <%= item.quantity %>
  </td>
  <td>
  $<%= item.total_price%>
  </td> 
</tr>
<% end %>